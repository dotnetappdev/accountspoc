{
  "ConnectionStrings": {
    "DefaultConnection": "Data Source=AccountsPOC.db",
    "UseConfiguredConnection": false
  },
  "DatabaseSetup": {
    "AllowRuntimeConfiguration": true,
    "ConfigFilePath": "database-config.json"
  },
  "Sentry": {
    "Dsn": "",
    "Environment": "Development",
    "TracesSampleRate": 1.0,
    "SendDefaultPii": false,
    "MaxBreadcrumbs": 100,
    "Debug": false,
    "DiagnosticLevel": "Error",
    "AttachStacktrace": true
  },
  "Jwt": {
    "Key": "SuperSecretKeyForDevelopmentOnlyMustBe32BytesOrMoreForHS256Algorithm1234567890",
    "Issuer": "AccountsPOC",
    "Audience": "AccountsPOCApp",
    "ExpireDays": "7"
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*"
}
